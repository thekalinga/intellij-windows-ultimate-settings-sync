<application>
  <component name="CodeInsightSettings">
    <EXCLUDED_PACKAGE NAME="java.applet.*" />
    <EXCLUDED_PACKAGE NAME="java.awt.*" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>