    </template>
    <template id="mockito@userDefined" key=".mockito" provider="builtin.java" language-level="8" topmost="false">
      <conditions>
        <condition id="fqn" fqn="java.lang.Class" />
      </conditions>
      <template name="fakeKey" value="Mockito.mock($EXPR$);" description="" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="thenReturn@userDefined" key=".thenReturn" provider="builtin.java" language-level="8" topmost="false">
      <conditions>
        <condition id="non void" />
      </conditions>
      <template name="fakeKey" value="Mockito.when($EXPR$).thenReturn($END$);" description="" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="d@userDefined" key=".d" provider="builtin.java" language-level="8" topmost="false">
