<application>
  <component name="PostfixTemplates">
    <template id="ati@userDefined" key=".ati" provider="builtin.java" language-level="8" topmost="false">
      <conditions>
        <condition id="non void" />
      </conditions>
      <template name="fakeKey" value="assertThat($EXPR$).is" description="" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="at@userDefined" key=".at" provider="builtin.java" language-level="8" topmost="false">
      <conditions>
        <condition id="non void" />
      </conditions>
      <template name="fakeKey" value="assertThat($EXPR$)." description="" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="w@userDefined" key=".w" provider="builtin.java" language-level="8" topmost="false">
      <conditions>
        <condition id="non void" />
      </conditions>
      <template name="fakeKey" value="log.warn($EXPR$$END$);" description="" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="e@userDefined" key=".e" provider="builtin.java" language-level="8" topmost="false">
      <conditions>
        <condition id="non void" />
      </conditions>
      <template name="fakeKey" value="log.error($EXPR$$END$);" description="" toReformat="true" toShortenFQNames="true" />
