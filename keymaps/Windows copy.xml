<keymap version="1" name="Windows copy" parent="$default">
  <action id="CloseActiveTab">
    <keyboard-shortcut first-keystroke="shift ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl d" />
  </action>
  <action id="EditorDuplicate">
    <keyboard-shortcut first-keystroke="shift ctrl d" />
  </action>
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="shift ctrl period" />
  </action>
  <action id="EditorToggleCase">
    <keyboard-shortcut first-keystroke="shift back_quote" />
  </action>
  <action id="EditorUnSelectWord">
    <keyboard-shortcut first-keystroke="shift ctrl comma" />
  </action>
  <action id="Maven.Reimport">
    <keyboard-shortcut first-keystroke="shift ctrl alt m" />
  </action>
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="shift ctrl back_slash" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="ctrl back_slash" />
  </action>
  <action id="ToggleZenMode">
    <keyboard-shortcut first-keystroke="shift ctrl alt z" />
  </action>
</keymap>